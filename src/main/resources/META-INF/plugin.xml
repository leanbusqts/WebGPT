<idea-plugin>
    <id>dev.bulean.webgpt</id>
    <name>WebGPT</name>
    <vendor email="bulean@protonmail.com" url="https://www.bulean.dev">bulean</vendor>
    <depends>com.intellij.modules.platform</depends>
    <depends>org.jetbrains.kotlin</depends>
    <description>
        <![CDATA[Plugin to access ChatGPT Web from IntelliJ/Android Studio.]]>
    </description>
    <change-notes>
        <![CDATA[
          <ul>
            <li><b>1.0.0</b> Release version.</li>
          </ul>
        ]]>
    </change-notes>
    <extensions defaultExtensionNs="com.intellij">
        <toolWindow
            id="WebGPT"
            anchor="right"
            icon="/META-INF/webgpticon.svg"
            factoryClass="dev.bulean.webgpt.WebGPTToolWindowFactory"
            secondary="false"
            canCloseContents="false"/>
    </extensions>
</idea-plugin>
